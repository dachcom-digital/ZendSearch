{
    "name": "dachcom-digital/zendsearch",
    "description": "a general purpose text search engine written entirely in PHP 5",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "lucene"
    ],
    "autoload": {
        "psr-4": {
            "ZendSearch\\": "library/ZendSearch"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0"
    },
    "replace": {
        "zendframework/zendsearch": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
